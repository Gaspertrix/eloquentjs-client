  },
  "homepage": "https://github.com/parsnick/laravel-eloquentjs#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.3.3",
    "sinon": "^1.17.1",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^3.10.1"
